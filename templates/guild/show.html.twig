{% extends 'base.html.twig' %}

{% block title %}Guild{% endblock %}

{% block body %}
    <h1>Guild</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ guild.id }}</td>
            </tr>
            <tr>
                <th>Faction</th>
                <td>{{ guild.faction }}</td>
            </tr>
            <tr>
                <th>Server</th>
                <td>{{ guild.server }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ guild.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_guild_index') }}">back to list</a>

    <a href="{{ path('app_guild_edit', {'id': guild.id}) }}">edit</a>

    {{ include('guild/_delete_form.html.twig') }}
{% endblock %}
